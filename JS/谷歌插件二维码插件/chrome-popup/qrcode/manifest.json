{
  "name": "QRcode",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "根据链接或输入值生成二维码",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon.png"
    },
    "default_title": "二维码生成器",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.sojson.com/*",
    "tabs",
    "storage"
  ]
}